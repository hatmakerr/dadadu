<header>
  <div class="container">
    <div class="logo">
      {% if settings.logo != blank %}
        <img src="{{ settings.logo | img_url: '200x' }}" alt="{{ shop.name }}">
      {% else %}
        <h1>{{ shop.name }}</h1>
      {% endif %}
    </div>
    <nav>
      <ul>
        {% for link in linklists.main-menu.links %}
          <li>
            <a href="{{ link.url }}">{{ link.title }}</a>
          </li>
        {% endfor %}
      </ul>
    </nav>
  </div>
</header>